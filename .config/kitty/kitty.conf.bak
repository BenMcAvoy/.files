# BEGIN_KITTY_THEME
# Tokyo Night
include current-theme.conf
# END_KITTY_THEME

font_family      JetBrainsMono NerdFont
bold_font        auto
italic_font      auto
bold_italic_font auto

font_size 12.0

# Enable remote control and unix socket
allow_remote_control socket-only
listen_on unix:/tmp/kitty
